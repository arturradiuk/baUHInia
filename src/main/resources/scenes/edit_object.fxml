<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="boarderPane" maxHeight="-Infinity" prefHeight="200" prefWidth="600" stylesheets="/scenes/simulation_view.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="widthField: " />
                    <TextField fx:id="widthField" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="lengthField: " />
                    <TextField fx:id="lengthField" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="heightField: " />
                    <TextField fx:id="heightField" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="ObjectTypeField: " />
                    <TextField fx:id="objectTypeField" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="priceField: " />
                    <TextField fx:id="allowedTypeField" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="priceField: " />
                    <TextField fx:id="priceField" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="heatFactorField: " />
                    <TextField fx:id="heatFactorField" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <children>
                    <Label prefHeight="37.0" text="saveField: " />
                    <TextField fx:id="saveField" />
                    <Button mnemonicParsing="false" onMouseClicked="#browse" style="-fx-background-color: #70af85; -fx-border-color: #f0e2d0; -fx-border-width: 0;" text="Browse" textAlignment="CENTER" textFill="#f0e2d0" BorderPane.alignment="CENTER" />
                    <Button mnemonicParsing="false" onMouseClicked="#saveToFile" style="-fx-background-color: #70af85; -fx-border-color: #f0e2d0; -fx-border-width: 0;" text="Save" textAlignment="CENTER" textFill="#f0e2d0" BorderPane.alignment="CENTER" />
                </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
                <padding>
                    <Insets left="16.0" top="2.0" />
                </padding>
                <Button mnemonicParsing="false" onMouseClicked="#saveObject" style="-fx-background-color: #70af85; -fx-border-color: #f0e2d0; -fx-border-width: 0;" text="SaveObject" textAlignment="CENTER" textFill="#f0e2d0" BorderPane.alignment="CENTER" />
            </HBox>
        </VBox>
    </center>
</BorderPane>
